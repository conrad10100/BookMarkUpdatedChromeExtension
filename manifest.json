{
  "name": "My Bookmark Updater",
  "version": "0.1.0",
  "description": "Updates existing bookmarks with new URLs if similar bookmarks exist.",
  "permissions": ["storage", "tabs","bookmarks"],
  "host_permissions": ["https://*.youtube.com/*"],


  "action": {
    "default_icon": {
      "16": "assets/ext-icon.png",
      "24": "assets/ext-icon.png",
      "32": "assets/ext-icon.png"
    },
    "default_title": "My Bookmark Updater",
    "default_popup": "popup.html"
  },
  "manifest_version": 3
}
